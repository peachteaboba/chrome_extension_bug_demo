{
  "manifest_version": 2,
  "name": "Chrome Extension Bug Demo v2",
  "description": "Chrome Extension Bug Demo v2",
  "version": "2.00",
  "author": "Andy Feng",
  "browser_action": {
    "default_icon": "images/bug.png",
    "default_title": "Chrome Extension Bug Demo v2",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "storage",
    "tabs",
    "192.168.1.4:3000/*",
    "192.168.1.4:3001/*",
    "192.168.1.176:3000/*",
    "192.168.1.176:3001/*",
    "http://localhost:3001/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/packages/jquery-3.4.0.min.js",
        "js/content.js"
      ]
    },
    {
      "matches": [
        "http://localhost:3000/*",
        "http://192.168.1.176:3000/*",
        "http://192.168.1.4:3000/*"
      ],
      "js": [
        "js/packages/jquery-3.4.0.min.js",
        "js/rettro_content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "externally_connectable": {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "accept_tls_channel_id": true
  }
}
